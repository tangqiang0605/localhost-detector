{
  "manifest_version": 3,
  "name": "Localhost Port Detector",
  "version": "0.1",
  "description": "Detects and provides quick access to services running on localhost.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  },
  "host_permissions": [
    "http://localhost/"
  ]
}